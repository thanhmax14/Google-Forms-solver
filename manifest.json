{
  "manifest_version": 3,
  "name": "Form Answer Helper",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Form Helper"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "functions.js"
      ]
    }
  ]
}